<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>InicioSesion</servlet-name>
        <servlet-class>Servelt.InicioSesion</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>enviar</servlet-name>
        <servlet-class>Servelt.enviar</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>abrirMetodo</servlet-name>
        <servlet-class>Servelt.abrirMetodo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TraerIdRu</servlet-name>
        <servlet-class>Servelt.TraerIdRu</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>pantallaMetodo</servlet-name>
        <servlet-class>Servelt.pantallaMetodo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>mandarMetodo</servlet-name>
        <servlet-class>Servelt.mandarMetodo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>buscador</servlet-name>
        <servlet-class>Servelt.buscador</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>enviarDatos</servlet-name>
        <servlet-class>Servelt.enviarDatos</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>InicioSesion</servlet-name>
        <url-pattern>/iniciar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>enviar</servlet-name>
        <url-pattern>/enviar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>abrirMetodo</servlet-name>
        <url-pattern>/abrirMetodo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TraerIdRu</servlet-name>
        <url-pattern>/traeridru</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>pantallaMetodo</servlet-name>
        <url-pattern>/pantallaMetodo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>mandarMetodo</servlet-name>
        <url-pattern>/mandarMetodo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>buscador</servlet-name>
        <url-pattern>/buscador</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>enviarDatos</servlet-name>
        <url-pattern>/enviarDatos</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
